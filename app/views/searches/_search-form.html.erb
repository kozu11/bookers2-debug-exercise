  <% if user_signed_in? %>
      <%= form_tag(searches_path, method: :get) do %>
        <%= text_field_tag :search %>
        <%= select_tag "model", options_for_select({"User" => "user", "Book"=> "book"}) %>
        <%= select_tag "method", options_for_select({ "完全一致" => "perfect", "部分一致" => "partical", "後方一致" => "backward", "前方一致" => "forward"}) %>
        <%= submit_tag "検索" %>
      <% end %>
  <% end %>
